f get_terms_collection(quantity_of_variables, max_degree):\\n    if max_degree == 1:\\n        return [np.identity(quantity_of_variables, dtype=np.int32)]\\n\\n    # method 2 for implementing `terms_collection_s`: add a new arg 'all_pre_terms_collection' in function.\\n    terms_collection_for_certain_degree_list = []\\n    tcfcdl = terms_collection_for_certain_degree_list\\n    tcfcdl += get_terms_collection(quantity_of_variables, max_degree - 1)\\n\\n    terms_collection_for_one_smaller_degree = tcfcdl[-1]\\n    terms_collection_for_cur_max_degree = []\\n\\n    for idx in range(quantity_of_variables):\\n        for pre_term in terms_collection_for_one_smaller_degree:\\n            pre_term_ = pre_term.copy()\\n            pre_term_[idx] += 1\\n            terms_collection_for_cur_max_degree.append(pre_term_)\\n        # generate `term_collector` by filtering something like [1, 2], [2, 1] by set? seems wrong.\\n        terms_collection_for_one_smaller_degree = list(\\n            filter(lambda term: term[idx] == 0, terms_collection_for_one_smaller_degree)\\n        )\\n    tcfcdl.append(np.array(terms_collection_for_cur_max_degree))\\n    return tcfcdl\\n\\n\\n# where to put the calculation of coefficients?\\ndef polynomial_function(*variables, max_degree=3, coefficients=None, custom_terms_collection=None, iterating=False):\\n    term_value_calculator = lambda term: coefficients.pop(0) * sum(\\n        var ** power if power != 0 else 0 for var, power in zip(variables, term)\\n    )\\n    \\n    if custom_terms_collection is not None:\\n        terms_collection = custom_terms_collection\\n    else:\\n        quantity_of_variables = len(variables)\\n        terms_collection = get_terms_collection(quantity_of_variables, max_degree)\\n    \\n    def term_value_generator():\\n        for term in np.vstack(terms_collection): \\n            yield term_value_calculator(term)\\n    \\n    if coefficients is None:\\n        # the `1` corresponds to the bias.\\n        coefficients = list(np.random.random((\\n            sum(len(_) for _ in terms_collection) + 1,\\n        )))\\n    if iterating: \\n        return term_value_generator()\\n    else:\\n        output = 0\\n        for terms_collection_for_certain_degree in terms_collection:\\n            for term in terms_collection_for_certain_degree:\\n                    output += term_value_calculator(term)        \\n        return output\\n    \\n\\n\\n# def polynomial_function(feature_input_s, max_degree=3, coefficients_fadins=None, custom_terms_collection=None, iterating=False):\\n#     term_value_calculator = lambda \\n    \\n#     if custom_terms_collection is not None:\\n#         terms_collection = custom_terms_collection\\n#     else:\\n#         quantity_of_variables = len(variables)\\n#         terms_collection = get_terms_collection(quantity_of_variables, max_degree)\\n    \\n#     def term_value_generator():\\n#         for term in np.vstack(terms_collection): \\n#             yield term_value_calculator(term)\\n    \\n#     if coefficients is None:\\n#         # the `1` corresponds to the bias.\\n#         coefficients = list(np.random.random((\\n#             sum(len(_) for _ in terms_collection) + 1,\\n#         )))\\n#     if iterating: \\n#         return term_value_generator()\\n#     else:\\n#         output = 0\\n#         for terms_collection_for_certain_degree in terms_collection:\\n#             for term in terms_collection_for_certain_degree:\\n#                     output += term_value_calculator(term)        \\n#         return output\\n\\ng = polynomial_function(*[1,2,3], max_degree=1, coefficients=[0.5, 0.5, 0.5, 0], iterating=False)\\nprint(g)\\n# print(next(g))\\n# print(next(g))\\n# a = get_terms_collection(2, 3)\\n# print(a)\\n# print(np.vstack(a))\\n\", \"outputs\": [{\"output_type\": \"stream\", \"name\": \"stdout\", \"text\": \"3.0\\n\"}], \"execution_count\": 130, \"cell_type\": \"code\"}, {\"metadata\": {\"code_folding\": [], \"trusted\": true}, \"source\": \"\\n    \\nclass ENN():\\n    def __init__(self, config):\\n        self.config = config\\n          \\n        # `fp_trasformation`: feature_polynomial_transformation\\n        self.fp_transformation  = np.vstack(get_terms_collection(\\n            self.config.feature_dimension, self.config.max_degree))\\n        \\n        # `W`: coefficients_for_all_dimension_in_next_space\\n        # len of `fp_transformation`: quantity_of_variable_term\\n        self.W = np.random.normal(size=(\\n            len(self.fp_transformation), self.config.space_mapping_process[1]))\\n        \\n        # `b`: constant_coefficient_for_all_dimension_in_next_space\\n        self.b = np.random.random((self.config.space_mapping_process[1], ))\\n        \\n    def feature_transform(self, feature_input_s):\\n        feature_output_s = []\\n        for x in feature_input_s:\\n            feature_output_s.append([\\n                sum(var ** power if power != 0 else 0 for var, power in zip(x, term)) \\n                    for term in self.fp_transformation\\n            ])            \\n        return np.array(feature_output_s)\\n            \\n    # backpropagation\\n    # note that `x_s_train` is just an alias of `feature_input_s`.\\n    def fit(self, x_s_train, y_s_train, params_trace_recording=False):\\n        x_s_train, y_s_train = np.array(x_s_train), np.array(y_s_train)\\n        W_trace, b_trace = [self.W], [self.b]\\n        sample_size = len(x_s_train)\\n        \\n        for step in range(self.config.steps):\\n            indices_batch = random.sample(range(sample_size), self.config.batch_size)\\n            \\n            # i corresponds to row in `self.W` and j to col.\\n            # define them as function without actual object reference can free memory at soon. and I \\\\\\n            # think it's faster than del statement.\\n            all_xi = self.feature_transform(x_s_train[indices_batch])\\n            all_yj_minus_fj = lambda : all_xi.dot(self.W) + self.b - y_s_train[indices_batch] \\n            all_yj_minus_one = lambda : y_s_train[indices_batch] - 1\\n            \\n            # a more clear writing style:\\n            # gradient_ij = np.mean(all_yj_minus_fj[:, j]) \\n            #     * np.mean(all_yj_minus_one[:, j]) \\n            #     * np.mean(all_xi[:, i])\\n            _ = np.mean(all_yj_minus_fj(), 0, keepdims=True) * np.mean(all_yj_minus_one(), 0, keepdims=True)\\n            # or `... np.mean(all_xi, 0, keepdims=True).T ...`\\n            self.W -= self.config.learning_rate * np.mean(all_xi, 0)[:, np.newaxis].dot(_)\\n            self.b -= self.config.learning_rate * _.flatten()\\n            if step % 50 == 0:\\n                print(self.W)\\n                print(self.b)\\n                print()\\n            if params_trace_recording:\\n                W_trace.append(self.W)\\n                b_trace.append(self.b)\\n        print('training completed.') \\n        return W_trace, b_trace\\n\\n    # forward computing\\n    def predict(self, x_s, custom_params=None):\\n        if custom_params:\\n            return np.argmax(self.feature_transform(x_s).dot(custom_params['W']) + custom_params['b'], axis=1)\\n        else:    \\n            return np.argmax(self.feature_transform(x_s).dot(self.W) + self.b, axis=1)\\n\\n    \\n# data preparation.\\nsampling_size = 50\\nx = np.linspace(-1, 1, sampling_size)\\ny_1 = 0.5 * np.sin(3.1*(x-0.5)) + 0.4\\ny_2 = 0.5 * np.sin(3.1*(x-0.5)) - 0.45\\n\\nx_s_train = np.array(\\n    list(zip(x, y_1)) +\\n    list(zip(x, y_2))\\n)\\ny_s_train = np.array([[1, 0]]*50 + [[0, 1]]*50)\\n    \\n\\n# for W in W_trace:\\n#     print(W)\", \"outputs\": [], \"execution_count\": 146, \"cell_type\": \"code\"}, {\"metadata\": {\"trusted\": true}, \"source\": \"class config():\\n    feature_dimension = 2\\n    space_mapping_process = (feature_dimension, 2)\\n    max_degree = 1\\n    \\n    learning_rate = 1\\n    batch_size = 100\\n    steps = 1000\\n\\nenn = ENN(config)    \\nW_t